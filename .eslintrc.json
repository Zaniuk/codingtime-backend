{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": [
    "standard-with-typescript",
    "eslint-config-prettier"
  ],
  "plugins": ["prettier"],
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module",
    "project": "./tsconfig.json"
  },
  "ignorePatterns" : ["node_modules/*", ".next/*", ".out/*", "!.prettierrc.js", "data/*"],
  "rules": {
    "prettier/prettier": "warn"
  }
}
